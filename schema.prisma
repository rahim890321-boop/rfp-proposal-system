generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Proposal {
  id             String   @id @default(uuid())
  name           String
  summary        String?
  status         String   @default("DRAFT")
  timerStartedAt DateTime?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
